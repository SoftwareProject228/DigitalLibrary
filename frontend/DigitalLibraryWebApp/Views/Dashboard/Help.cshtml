@model CommandViewModel
@{
    ViewData["Title"] = "Help";
}
@section Scripts
{
    <link rel="stylesheet" href="~/css/help.css">
    <script src="~/js/jquery-3.4.1.min.js"></script>
    <script src="~/js/help.js"></script>
}

<main>

    <section id="help">
        <p><span>&raquo;Help?</span></p>
        <p><span>Type [command] + <kbd>Enter</kbd></span></p>
        <ul>
            <li>'home' -- Thats obvious!</li>
            <li>'library' -- List of available books</li>
            <li>'help' -- List of availale commands</li>
            <li>'upload' -- Create new post</li>
        </ul>
    </section>

    <form method="post">
        <span class="command">Vasilisa:/$
            <input name="CommandR" type="text">
            <input type="submit" hidden>
        </span>
    </form>

</main>